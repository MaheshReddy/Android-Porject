<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.es.basic"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".BasicActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.es.flows.Flow1" >
        </activity>
        <activity android:name="com.es.flows.SensorActivity" >
        </activity>
        <activity android:name="com.es.services.SocialLibActivity">
            <intent-filter><action android:name="android.intent.action.VIEW" /><category android:name="android.intent.category.DEFAULT" /><category android:name="android.intent.category.BROWSABLE" /><data android:scheme="proapp" android:host="sociallib.com" /></intent-filter>
        </activity>
        <activity android:name="com.es.services.GoogleCal"></activity>
        <activity android:name="com.es.services.FacebookApp1Activity"></activity>
        <activity android:name="com.es.services.Email"></activity>
    </application>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INTERNET" />  
</manifest>